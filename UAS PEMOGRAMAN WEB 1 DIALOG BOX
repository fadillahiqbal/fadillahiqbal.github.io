<section id="dialogbox">
        <center>
            <h2>INI DIALOG BOX SAYA FADILLAH IQBAL DALAM HTML</h2>
         </center>
         <button onclick="showDialog()">BUka Dialog</button>

<dialog id="myDialog">
    <p>Ini merupakan contoh dialog box.</p>
    <button onclick="closeDialog()">Close</button>
</dialog>

<script>
    function showDialog() {
        if (typeof HTMLDialogElement === 'function') {
            var myDialog = document.getElementById('myDialog');
            
            // Show the dialog box
            myDialog.showModal();
        } else {
            alert('Your browser does not support the dialog element. Please update your browser.');
        }
    }

    function closeDialog() {
        var myDialog = document.getElementById('myDialog');

        myDialog.close();
    }
    input {
            width: 50px;
            height: 50px;
            font-size: 18px;
            margin: 5px;
        }

        button {
            width: 50px;
            height: 50px;
            font-size: 18px;
            margin: 5px;
        }
